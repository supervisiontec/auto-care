	<parameter name="CURRENT_BRANCH" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[1]]></defaultValueExpression>
	</parameter>
	<parameter name="FROM_DATE" class="java.util.Date">
		<defaultValueExpression><![CDATA[2017-06-30]]></defaultValueExpression>
	</parameter>
	<parameter name="TO_DATE" class="java.util.Date">
		<defaultValueExpression><![CDATA[2017-06-30]]></defaultValueExpression>
	</parameter>
	<parameter name="BAY" class="java.lang.Integer"/>
	<parameter name="STATUS" class="java.lang.String"/>
	<queryString>
		<![CDATA[select 
	m_bay.name as bay_name,
	m_vehicle.vehicle_no,
	t_vehicle_assignment.in_time,
	t_vehicle_assignment.out_time,
	DATE_FORMAT((TIMEDIFF(t_vehicle_assignment.out_time, t_vehicle_assignment.in_time)), '%T')as spend_time ,
	m_branch.name as branch_name,
	IFNULL((SELECT SEC_TO_TIME(SUM( TIME_TO_SEC((TIMEDIFF(v.out_time, v.in_time)))))  
		FROM t_vehicle_assignment v
		WHERE
			m_bay.index_no=v.bay 
			and v.date BETWEEN   $P{FROM_DATE} and   $P{TO_DATE} ) ,'Pending')as total_time
FROM m_bay
	left JOIN t_vehicle_assignment on t_vehicle_assignment.bay=m_bay.index_no
	left JOIN t_job_card on t_job_card.index_no=t_vehicle_assignment.job_card
	left JOIN m_vehicle on m_vehicle.index_no=t_job_card.vehicle
	left JOIN m_branch on m_branch.index_no =m_bay.branch
WHERE
	m_bay.branch = $P{CURRENT_BRANCH}  and m_bay.vehicle_is_view=true
	and t_vehicle_assignment.date  >=  $P{FROM_DATE} and t_vehicle_assignment.date <=  $P{TO_DATE} 
	and ( $P{BAY}  is null or m_bay.index_no= $P{BAY} )
	and ( $P{STATUS}  is null or( $P{STATUS} ='PENDING' and t_job_card.out_time is  NULL) or $P{STATUS} ='COMPLETED' and t_job_card.out_time is not NULL) 
order by m_bay.index_no,m_vehicle.vehicle_no,t_vehicle_assignment.in_time


]]>
	</queryString>
